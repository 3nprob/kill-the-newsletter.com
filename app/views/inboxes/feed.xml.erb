<?xml version="1.0" encoding="UTF-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <link rel="self" type="application/atom+xml" href="<%= inbox_feed_url @inbox %>"/>
  <link rel="alternate" type="text/html" href="<%= root_url %>"/>
  <title><%= @inbox.name %></title>
  <subtitle>Kill the Newsletter! inbox “<%= inbox_email_address @inbox %>”</subtitle>
  <id><%= urn @inbox.id %></id>
  <%=
    render Entry.new(
      title: "“#{@inbox.name}” inbox created", 
      author: 'Kill the Newsletter!', 
      content: render(@inbox).to_str, 
      content_type: 'html'
    )
  %>
</feed>
